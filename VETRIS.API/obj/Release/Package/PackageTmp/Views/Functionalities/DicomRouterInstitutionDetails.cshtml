@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DICOM Router Institution Details</title>
    <link rel="shortcut icon" type="image/x-icon" href="../favicon.ico" />
    <link href="@Url.Content("~/CSS/Functionality.css")" rel="stylesheet" />

     <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script type="text/javascript">
        $(function () {
            $(document).on('click', '#btnSubmit', function () {
                $.ajax({
                    type: "POST",
                    url: document.getElementById("hdnURL").value + "DicomRouterInstitutionDetails",
                    data: {
                        "institutionCode": document.getElementById("txtinstitutionCode").value,
                    },
                    success: function (data) {
                        //alert(data.InstitutionName);
                        //alert(data.Address_1);
                        //alert(data.Address_2);
                        //alert(data.Zip);
                        //alert(data.InstitutionLoginID);
                        //alert(data.StudyImageFilesReceivingPath);
                        //alert(data.responseStatus.responseMessage);

                        if (document.all) {
                            document.getElementById("resInstitutionName").innerText = data.InstitutionName;
                            document.getElementById("resAddress1").innerText = data.Address_1;
                            document.getElementById("resAddress2").innerText = data.Address_2;
                            document.getElementById("resZip").innerText = data.Zip;
                            document.getElementById("resInstitutionLoginID").innerText = data.InstitutionLoginID;
                            ocument.getElementById("resStudyImageFilesReceivingPath").innerText = data.StudyImageFilesReceivingPath;
                            document.getElementById("resMsg").innerText = data.responseStatus.responseMessage;
                        }
                        else {
                            document.getElementById("resInstitutionName").textContent = data.InstitutionName;
                            document.getElementById("resAddress1").textContent = data.Address_1;
                            document.getElementById("resAddress2").textContent = data.Address_2;
                            document.getElementById("resZip").textContent = data.Zip;
                            document.getElementById("resInstitutionLoginID").textContent = data.InstitutionLoginID;
                            document.getElementById("resStudyImageFilesReceivingPath").textContent = data.StudyImageFilesReceivingPath;
                            document.getElementById("resMsg").textContent = data.responseStatus.responseMessage;
                        }
                        
                        
                    }
                });

            });
        });
        
    </script>
</head>
<body>
    <div class="hdr">
        VETRIS API : Get DICOM Router's Institution Details
    </div>
    <div style="height: 5px;"></div>
    <div id="desc" style="height: 350px;">
        <p>
            <b>Objective :</b> To get the institution details of DICOM ROUTER being installed/updated 
        </p>
        <p>
            The request object : <b>"DicomRouterInstitutionDetails"</b>. The response object : <b>"DicomRouterInstitutionDetailsResult"</b>
            
        </p>
        <div class="blocks" style="width:30%;">
            <b><u>Request Input Parameters</u></b>
            <div class="clr"></div>
            <div class="hdrsL" style="width:40%;">Parameter</div>
            <div class="hdrsR">&nbsp;&nbsp;Data Type</div>
            <div class="clr"></div>
            <div class="itmFL" style="width:40%;">institutionCode</div>
            <div class="itmFR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
           
        </div>
        <div class="blocks" style="margin-left: 10px;width:30%;">
            <b><u>Response Output Values</u></b>
            <div class="clr"></div>
            <div class="hdrsL" style="width:50%;">Value</div>
            <div class="hdrsR">&nbsp;&nbsp;Data Type</div>
            <div class="clr"></div>
            <div class="itmFL" style="width:50%;">InstitutionName</div>
            <div class="itmFR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
            <div class="itmL" style="width:50%;">Address_1</div>
            <div class="itmR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
            <div class="itmL" style="width:50%;">Address_2</div>
            <div class="itmR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
            <div class="itmL" style="width:50%;">Zip</div>
            <div class="itmR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
             <div class="itmL" style="width:50%;">InstitutionLoginID</div>
            <div class="itmR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
            <div class="clr"></div>
             <div class="itmL" style="width:50%;">StudyImageFilesReceivingPath</div>
            <div class="itmR">&nbsp;&nbsp;String</div>
            <div class="clr"></div>
            
        </div>
         <div style="margin-top: 50px;margin-top :50px;float:right;border:1px solid #000;width :100px;background-color:#eee;text-align:center;padding:10px;">
            <a href ="@Url.Action("Index","Home")" id="btnBack" style="text-decoration:none;" ><< Back </a> 
        </div>
        <div class="clr"></div>

    </div>
    <div id="req" style="height: 500px;width:40%;">
        <b><u>Request</u></b>

        <div style="margin-top: 20px;">institutionCode</div>
        <div>
            <input type="text" id="txtinstitutionCode" maxlength="5" />
        </div>
        
        <div style="margin-top: 30px;">
            <input type="button" id="btnSubmit" value="Submit >>" />
        </div>
    </div>
    <div id="res" style="height: 500px;width:57%;margin-left:10px;">
        <b><u>Response</u></b>
        <div style="margin-top: 10px;">InstitutionName</div>
        <div>
            <span id="resInstitutionName"></span>
        </div>
        <div style="margin-top: 10px;">Address_1</div>
        <div>
            <span id="resAddress1"></span>
        </div>
        <div style="margin-top: 10px;">Address_2</div>
        <div>
            <span id="resAddress2"></span>
        </div>
        <div style="margin-top: 10px;">Zip</div>
        <div>
            <span id="resZip"></span>
        </div>
        <div style="margin-top: 10px;">InstitutionLoginID</div>
        <div>
            <span id="resInstitutionLoginID"></span>
        </div>
        <div style="margin-top: 10px;">StudyImageFilesReceivingPath</div>
        <div>
            <span id="resStudyImageFilesReceivingPath"></span>
        </div>
        
        <div style="margin-top: 20px;">
            <b><u>Response Status</u></b>
        </div>
     
        <div style="margin-top: 10px;">responseMessage</div>
        <div>
            <span id="resMsg" style="color: #ff0000;"></span>
        </div>
    </div>
    <div class="clr"></div>
    <div class="ftr" style="margin-top:20px;">
        Copyright &copy; 2019 RAD365. All rights reserved.
    </div>
    <input type="hidden" id="hdnURL" value="@ViewBag.PublishURL" />
</body>
</html>

